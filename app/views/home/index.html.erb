
<section class="module parallax parallax-1">
  <div class="container">
    <h1>City Finder</h1>
  </div>
</section>

<section class="module content">
  <div class="container">
    <h2>Search</h2>
    <br>
    <div class="form-group">
      <label class= "col-md-1">
        City
      </label>
      <input type="text" name="city" placeholder="ex. Boston">
      <br>
      <label class="col-md-1">State</label>
        <%= render 'state_dropdown' %>
      <br>
      <br>
        <button class="btn btn-primary col-md-2 col-md-offset-1">
          Search
        </button>
    </div>
  </div>
</section>

<section class="module parallax parallax-2">
  <div class="container">
    <h1>Search</h1>
  </div>

</section>

<section class="module content">
  <div class="container">
    <div class="weather icon">
    </div>
    <div class="photos">
    </div>
  </div>
</section>
